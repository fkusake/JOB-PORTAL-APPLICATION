{
    "version": 2,
    "builds": [
      {
     "src": "./index.js",
     "use": "@vercel/node"
      }
    ],
    "routes": [
      {
     "src": "/(.*)",
     "dest": "/index.js"
      },
      { "src": "/(.*)", "dest": "/public" },
      { "src": "/pdf/(.*)", "dest": "/public/pdf/$1" }
    ]
    
     }